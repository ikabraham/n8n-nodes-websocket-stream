{
  "name": "n8n-nodes-websocket-stream",
  "version": "0.1.0",
  "description": "A production-ready custom n8n node for continuously streaming data from a WebSocket connection",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "websocket",
    "stream",
    "comfyui"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ikabraham/n8n-nodes-websocket-stream#readme",
  "author": {
    "name": "",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikabraham/n8n-nodes-websocket-stream.git"
  },
  "engines": {
    "node": ">=18.10.0",
    "pnpm": ">=9.1.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint"
  },
  "files": [
    "dist",
    "nodes",
    "package.json"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/WebSocketStream/WebSocketStream.node.js"
    ],
    "credentials": []
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": ">=1.0.0",
    "prettier": "^3.2.0",
    "typescript": "~5.3.3"
  }
}
