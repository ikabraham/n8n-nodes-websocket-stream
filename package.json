{
  "name": "n8n-nodes-websocket-stream",
  "version": "0.1.0",
  "description": "A production-ready custom n8n node for continuously streaming data from a WebSocket connection",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "websocket",
    "stream"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/n8n-nodes-websocket-stream.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "main": "dist/nodes/WebSocketStream.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/WebSocketStream.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "@types/node": "^18.0.0",
    "@langchain/core": "^0.0.1",
    "express": "^4.18.2",
    "nock": "^13.2.9",
    "ssh2": "^1.11.0",
    "@sentry/node": "^7.69.0"
  }
}
